
import std.*;

public proc void foo(chan<int>.read r) {
    int d;
    d = r.read();
    println(" > " + d);
}

public proc void bar(chan<int>.write w) {
    w.write(3);
}

public proc void main(string args[]) {
    chan<int> c;
    barrier b, bb, bbb;
    par enroll (b, bb, bbb) {
        foo(c.read);
        bar(c.write);
    }
}
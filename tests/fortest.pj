
import std.io;

proc void fooI(int i) { println("i = " + i); }
proc void fooJ(int j) { println("j = " + j); }
proc void fooK(int k) { println("K = " + k); }

public proc void main(string[] args) {
    println("Hi from main!");

    for (int i = 0, j = 0, k = 1; (i < 4) && (j < 10); i++, j = j + 3 + i, k = k + 2) {
        fooI(i);
        fooJ(j);
        fooK(k);
    }
}
import std.io;

record T {
    int a;
}

record K {
    int z;
    //T t;
}

record X extends T {
    int p;
    string b;
}

record P extends K, X, T {
    int x;
    int y;
}

record L {
    K k;
    string str;
}

public proc void writer(chan<L>.write out) {
  K k = new K { z = 3 };
  X x = new X { b = "Ben", p = 300, a = 20 };
  L l = new L { str = "Benjamin", k = new K { z = 56 } };

  out.write(l);
}

public proc void reader(chan<L>.read in) {
  L value;
  value = in.read();
  println("The value is " + value.k.z);
}

public proc void main(string args[]) {
  chan<L> c;
  int a = 2;
  par {
    writer(c.write);
    reader(c.read);
  }
}